<h1>Here are all of our users:</h1>

<table style="border:2px">
	<tr>
		<th>Name</th>
		<th>Age</th>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= link_to user.name, show_user_path(id: user.id) %></td>
		<td><%= user.age %></td>
	</tr>

<% end%>


</table>

<%= link_to "Return to Index", root_path %>